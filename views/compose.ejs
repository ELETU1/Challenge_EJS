<%- include("./partials/header") -%>

<h1>Abu Roslaan Compose Page</h1>

<form class="" action="/compose" method="post">
  <div class="form-group">
    <label>Title</label>
    <input type="text" class="form-control" name="postTitle" value="">
  </div>

  <div class="form-group">
    <label>Post</label>
    <textarea class="form-control"  rows="3" name="postBody"></textarea>
  </div>

  <button type="submit" class="btn btn-primary m-2" name="publish">Publish</button>
</form>

